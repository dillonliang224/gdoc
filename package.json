{
	"name": "zsdoc",
	"displayName": "zsdoc",
	"description": "zs apdoc tools",
	"publisher": "zsdoc", 
	"version": "0.0.3",
	"engines": {
		"vscode": "^1.30.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:extension.zsdoc"
	],
	"main": "./extension.js",
	"contributes": {
		"commands": [
			{
				"command": "extension.zsdoc",
				"title": "zsdoc"
			}
		],
		"menus": {
            "editor/context": [
                {
                    "when": "editorHasSelection",
                    "command": "extension.zsdoc"
                }
            ]
		},
		"keybindings": [
            {
                "command": "extension.zsdoc",
                "key": "ctrl+shift+d",
                "mac": "ctrl+alt+z",
                "when": "editorHasSelection"
            }
        ]
	},
	"scripts": {
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"eslint": "^4.11.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"lodash": "^4.17.11"
	}
}
